+cc='g++'
+lib=m
+cc_flags='-Wall -I/home/nort/SW/le-icosfit/git/levmar-2.6'
+lib=levmar +lib_sp=(/home/nort/SW/le-icosfit/build-levmar)
+lib=lapack
+lib=blas

%dflags == <<
+cc='g++'
+lib=m
+cc_flags='-I/home/nort/SW/le-icosfit/git/levmar-2.6 -g'
+lib=levmar +lib_sp=(/home/nort/SW/le-icosfit/build-levmar)
+lib=lapack
+lib=blas

%help ! == %target :help
%target == icosfit.specs +(%flags) :exe
%dtarget == icosfit.specs +(%dflags) +debug :exe
